{
  "expo": {
    "name": "Bitcoin Blockchain Explorer",
    "slug": "bitcoin-blockchain-explorer-android",
    "description": "An unofficial app to explore the Bitcoin blockchain",
    "privacy": "hidden",
    "version": "1.0.0",
    "platforms": [
      "android"
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#111428"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#111428"
      },
      "package": "com.pedrow21.bitcoinblockchainexplorerandroid"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cda97b11-945f-4934-8b4b-1031a7e48bdf"
      }
    },
    "plugins": [
      "expo-localization"
    ]
  }
}
